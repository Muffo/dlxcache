l1: addi r1,r0,0 ; azzera r1
l2: addi r2,r2,1 ; incrementa r2
l3: sw 10(r1),r2 ; memorizzza il valore 1 all'indirizzo 10
l4: sw 14(r1),r2 ; memorizzza il valore 1 all'indirizzo 14
l5: lw r3,10(r1) ; porta in r3 il valore all'indirizzo 10
l6: lw r4,14(r1) ; porta in r4 il valore all'indirizzo 14
l7: j l2         ; salta a l2