l1: addi r1,r0,0 ; azzera r1
l2: addi r2,r0,1 ; imposta a 1 r2
l3: addi r3,r0,1 ; imposta a 1 r3
l4: sw 10(r1),r2 ; memorizzza il valore di r2 all'indirizzo 10
l5: addi r2,r3,1 ; assegna a r2 il valore di r3 incrementato
l6: lw r3,10(r1) ; carica in r3 il valore di r2 prima dell'incremento
l7: j l4         ; salta a l4
